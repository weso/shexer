@prefix : <http://weso.es/shapes/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix shexer: <http://weso.es/shexer/ontology/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Person a sh:NodeShape ;
    shexer:total_conforming_instances 5 ;
    sh:property [ a sh:PropertyShape ;
            shexer:total_conforming_instances 2 ;
            rdfs:comment "# 2 instances. obj: xsd:string. Cardinality: {1}" ;
            sh:dataType xsd:string ;
            sh:maxCount 1 ;
            sh:path foaf:familyName ],
        [ a sh:PropertyShape ;
            shexer:total_conforming_instances 4 ;
            rdfs:comment "# 4 instances. obj: xsd:integer. Cardinality: {1}" ;
            sh:dataType xsd:integer ;
            sh:maxCount 1 ;
            sh:path foaf:age ],
        [ a sh:PropertyShape ;
            shexer:total_conforming_instances 1 ;
            rdfs:comment "# 1 instance. obj: @:Person. Cardinality: {1}" ;
            sh:maxCount 1 ;
            sh:node :Person ;
            sh:path foaf:knows ],
        [ a sh:PropertyShape ;
            shexer:total_conforming_instances 5 ;
            sh:in ( foaf:Person ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdf:type ],
        [ a sh:PropertyShape ;
            shexer:total_conforming_instances 3 ;
            rdfs:comment "# 3 instances. obj: xsd:string. Cardinality: {1}" ;
            sh:dataType xsd:string ;
            sh:maxCount 1 ;
            sh:path foaf:name ] ;
    sh:targetClass foaf:Person .